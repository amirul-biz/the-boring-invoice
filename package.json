{
  "scripts": {
    "postinstall": "prisma generate",
    "build": "prisma generate && npx nx build api --prod",
    "db:migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env.production -- prisma migrate deploy",
    "db:migrate:status": "dotenv -e .env.production -- prisma migrate status",
    "db:studio:dev": "dotenv -e .env.development -- prisma studio",
    "db:studio:prod": "dotenv -e .env.production -- prisma studio",
    "db:push:dev": "dotenv -e .env.development -- prisma db push",
    "db:push:prod": "dotenv -e .env.production -- prisma db push",
    "serve:dev": "nx serve api --configuration=development",
    "serve:prod": "nx serve api --configuration=production"
  },
